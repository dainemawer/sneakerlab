{% comment %}
    Adding section for Sneaker Lab header
{% endcomment %}
<link rel="stylesheet" href="{{ 'component-cart-notification.css' | asset_url }}" media="print" onload="this.media='all'">
<noscript>{{ 'component-cart-notification.css' | asset_url | stylesheet_tag }}</noscript>
<script src="{{ 'cart-notification.js' | asset_url }}" defer="defer"></script>

<a class="sr-only focus:not-sr-only" href="#main-content">Skip to Main Content</a>
<header aria-label="Site Header" role="banner" class="sticky bg-transparent z-10 top-[-1px] py-3 transition-all">
    <div class="menu-overlay"></div>
    <div class="flex flex-wrap justify-between px-4 lg:px-5 xl:container xl:mx-auto">
