                </svg>
            </a>
        </div>
        <nav class="flex justify-end basis-2/6">
            <ul class="flex">
                <li class="mr-6 text-sm hidden lg:inline-block"><a href="#search" aria-expanded="false" aria-controls="search" data-id="js-trigger-search" role="button">Search</a></li>
                <li class="mr-6 text-sm xl:hidden inline-block scale-75">
                    <a aria-label="Search Sneaker Lab" data-id="js-trigger-search" href="#search" role="button" aria-expanded="false" aria-controls="search">
                        <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" viewbox="0 0 20 20" fill="currentColor">
                            <path fill-rule="evenodd"
                                  d="M8 4a4 4 0 100 8 4 4 0 000-8zM2 8a6 6 0 1110.89 3.476l4.817 4.817a1 1 0 01-1.414 1.414l-4.816-4.816A6 6 0 012 8z"
                                  clip-rule="evenodd" />
                        </svg>
                    </a>
                </li>
                <li class="mr-6 text-sm hidden lg:inline-block">
                    {%- if customer -%}
                        <a
                            aria-expanded="false"
                            aria-controls="account-modal"
                            role="button"
                            data-logged-in="{%- if customer -%}true{%- else -%}false{%- endif -%}"
                            href="{{ routes.account_url }}"
                        >
                            My Account
                        </a>
                    {%- else -%}
                        <a
                            data-id="js-account-trigger"
                            aria-expanded="false"
                            aria-controls="account-modal"
                            role="button"
                            data-logged-in="{%- if customer -%}true{%- else -%}false{%- endif -%}"
                        >
                            Account
                        </a>
                    {%- endif -%}
                </li>
                <li class="text-sm">
                    <a href="{{ routes.cart_url }}" class="header__icon header__icon--cart link focus-inset" id="cart-icon-bubble">
                        <span class="visually-hidden">{{ 'templates.cart.cart' | t }}</span>
                        {%- if cart != empty -%}
                            <div class="cart-count-bubble">
                                {%- if cart.item_count < 300 -%}
                                    <span aria-hidden="true">Cart<sup id="cart-count">{{ cart.item_count }}</sup></span>
                                {%- endif -%}
                                <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                            </div>
                        {%- endif -%}
                    </a>
                </li>
            </ul>
        </nav>
        <div class="relative w-full">
            {% render 'header-search' %}
        </div>
    </div>
    {% render 'header-navigation' %}
    {% render 'account-modal' %}
</header>

{%- render 'cart-notification' -%}
